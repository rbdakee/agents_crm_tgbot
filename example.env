# Telegram bot config
BOT_TOKEN=your_bot_token_here
BOT_USERNAME=your_bot_username_here

# n8n endpoint URL returning/accepting JSON contracts
N8N_WEBHOOK_URL=https://n8n.example.com/webhook/sheet

# Webhook mode (set to true to enable webhook server)
USE_WEBHOOK=false
WEBHOOK_URL=https://your.domain.tld/bot
WEBAPP_HOST=0.0.0.0
WEBAPP_PORT=8080
WEBHOOK_PATH=/your_secret_path

# Logging
LOG_LEVEL=INFO
